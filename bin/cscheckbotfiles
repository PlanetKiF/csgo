#!/bin/bash
tmpfile=$(mktemp /tmp/csmapaliases.XXXXXX)

CSOWNER=%CSGO_USERNAME%
CSGODIR=/home/${CSOWNER}/csgosv
SOURCE=${CSGODIR}/csgo/cfg/botprofile.db.custom
TARGET=${CSGODIR}/csgo/cfg/botprofile.db
EXPECTED=${CSGODIR}/csgo/cfg/botprofile.db.lastorig
LOGFILE=/home/${CSOWNER}/updatelog.txt

DIFFRENCES=$(diff ${EXPECTED} ${TARGET})

if [ -ne ${DIFFRENCES} ];
    then echo "$(date)  Problem, could not update the 'botprofile.db'. The original file is modified (probably by an update.)" >> ${LOGFILE}
    else echo cp -f ${SOURCE} ${TARGET}
fi


